<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    style="@style/BaseListItemRootElement"
    android:orientation="horizontal">

    <TextView
        android:id="@+id/sensorTimeTV"
        style="@style/ExperimentViewCol1"
        tools:text="26/03/2022 22:37:25" />

    <TextView
        android:id="@+id/sensorValueTV"
        style="@style/ExperimentViewCol2"
        android:visibility="gone"
        tools:text="Tiempo" />

    <ImageView
        android:id="@+id/embeddingIV"
        style="@style/ExperimentViewCol2ImageView"

        android:adjustViewBounds="true"
        android:contentDescription="@string/experiment_view_data_embedding_check_content_description"
        app:srcCompat="@drawable/ic_baseline_check_24" />

    <ImageView
        android:id="@+id/syncIV"
        style="@style/ExperimentViewCol3ImageView"
        android:contentDescription="@string/experiment_view_data_embedding_check_content_description"
        app:srcCompat="@drawable/ic_baseline_check_24" />


</LinearLayout>